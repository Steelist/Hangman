<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fi.example.aleksi.hangman">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_hangman"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_hangman_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Multiplayer"></activity>
        <activity android:name=".Singleplayer"></activity>
        <activity android:name=".Highscore"></activity>
        <activity android:name=".Settings"></activity>
    </application>

</manifest>